#include <stdio.h>
#include <stdlib.h>

int main(void) {

	float C;

	printf("Input CulCe: ");
	scanf("%f", &C);

	printf("%.2f C = %.2f F \n", C, 9 / 5 * C + 32);

	return 0;
}
