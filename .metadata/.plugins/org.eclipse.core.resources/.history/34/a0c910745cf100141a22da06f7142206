#include <iostream>
#include <string>

using namespace std;

int main() {

	char charr1[20];
	char charr2[20] = "Jaguar";

	string str1;
	string str2 = "Honda";

	cout << "Cat Name: ";
	cin >> charr1;

	cout << "Another Cat Name: ";
	cin >> str1;

	cout << "The animals are all cat." << endl;
	cout << charr1 << " " << charr2 << " " << str1 << " " << str2 << endl;

	return 0;
}



