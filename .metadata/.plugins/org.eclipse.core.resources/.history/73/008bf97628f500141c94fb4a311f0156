#include <stdio.h>

#define MIN 0
#define MAX 100

int main(void) {

	int num, cnt = 0;
	int sum = 0;
	float avg;

	scanf("%d", &num);

	while (MIN < num && num < MAX) {
		sum += num;
		cnt++;
		scanf("%d", &num);
	}

	avg = sum / cnt;

	printf("Sum: %d, Avg: %.1f", sum, avg);

	return 0;
}


