#include <iostream>

using namespace std;

class Point
{
private:
	int m_x, m_y;

public:
	void setX(int x)
	{
		m_x = x;
	}

	void setY(int y)
	{
		m_y = y;
	}

	int getX()
	{
		return m_x;
	}

	int getY()
	{
		return m_y;
	}
};

int main()
{


	return 0;
}

