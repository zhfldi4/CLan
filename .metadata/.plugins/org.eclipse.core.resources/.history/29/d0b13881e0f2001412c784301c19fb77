#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int i = 30, cnt;

	printf("12345678901234567890\n");
	cnt = printf("i = %d", i);
	printf("\n printed characters count : %d\n ", cnt);

	return 0;
}
