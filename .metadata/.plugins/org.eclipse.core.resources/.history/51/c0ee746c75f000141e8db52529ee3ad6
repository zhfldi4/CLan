#include <iostream>
#include <string>

using namespace std;

void swap (int &a, int &b);

int main()
{
	int a, b;

	cout << "(a,b) = ";
	cin >> a >> b;

	swap (a, b);

	cout << "(" << a << ", " << b << ")" << endl;


	return 0;
}

void swap(int &a, int &b)
{
	int tmp = a;
	a = b;
	b = tmp;
}


