#include <iostream>
#include <string>

using namespace std;

int main()
{
	// file name
	string file;

	cout << "input file name : ";
	cin >> file;

	int idx = file.find_last_of('.');

	if (idx == string::npos)
		cout << "No Extractor file " << endl;
	else
		cout << "Extractor exist : " << file.substr(idx) << endl;

	return 0;
}


