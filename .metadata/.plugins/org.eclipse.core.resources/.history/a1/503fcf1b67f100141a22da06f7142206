#include <iostream>

using namespace std;


int main() {

	int smallNum, largeNum;

	cout << "Input two numbers: ";
	cin >> smallNum >> largeNum;

	int sum = 0;

	for (int i = smallNum; i < largeNum; i++) {
		sum += i;
	}

	cout << smallNum << " to " << largeNum << "'s sum is " << sum << endl;

	return 0;

}
