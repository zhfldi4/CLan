#include <iostream>
using namespace std;

template <class Any>
void Swap(Any &a, Any &b);

int main() {



	return 0;
}

template <class Any>
void Swap(Any &a, Any &b) {
	Any tmp;
	tmp = a;
	a = b;
	b = tmp;
}
