#include <iostream>

using namespace std;

int main() {

	int count;

	cout << "print row num: ";
	cin >> count;

	for (int i = 0; i < count; i++) {
		for (int j = 0; j < count; j++) {
			cout << '*';
		}
		cout << endl;
	}

	return 0;

}
