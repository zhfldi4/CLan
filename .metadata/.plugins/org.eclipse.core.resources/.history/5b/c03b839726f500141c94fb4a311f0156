#include <stdio.h>

int main(void) {

	int year;

	printf("Input year: " );
	scanf("%d", &year);

	if ((year % 400) == 0) {
		printf("leap year");
	} else if ((year % 4) == 0 && (year % 100) != 0) {
		printf("leap year");
	} else {
		printf("normal year");
	}

	return 0;
}


