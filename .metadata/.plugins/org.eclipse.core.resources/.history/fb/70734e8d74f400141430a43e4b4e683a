#include <iostream>
#include "mytime0.h"

using namespace std;

int main() {

	Time planning;
	Time coding(2, 40);
	time fixing(5, 55);
	Time total;

	cout << "Planning time: ";
	planning.show();
	cout << endl;

	cout << "Coding time: ";
	coding.show();
	cout << endl;

	cout << "Fixing time: ";
	fixing.show();
	cout << endl;

	total = coding.sum(fixing);

	return 0;
}


