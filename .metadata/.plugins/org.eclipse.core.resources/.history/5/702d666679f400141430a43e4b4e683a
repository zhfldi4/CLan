#include <iostream>
#include "stonewt.h"

using namespace std;

void display(const Stonewt &st, int n);

int main() {

	return 0;
}

void display(const Stonewt &st, int n) {
	for (int i = 0; i < n; i++) {
		cout << "Wow! ";
		st.show_stn();
	}
}

