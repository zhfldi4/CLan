#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int num1, num2;

	printf("Input two number: ");
	scanf("%d %d", &num1, &num2);

	printf("%d * %d = %d", num1, num2, num1 * num2);

	return 0;
}
