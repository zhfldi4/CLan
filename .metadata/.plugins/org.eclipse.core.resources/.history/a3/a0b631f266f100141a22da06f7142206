#include <iostream>
#include <string>
using namespace std;

struct CandyBar {
	string name;
	double weight;
	int cal;
};

int main() {

	CandyBar sneak = {"Mocha Munch", 2.3, 350};

	cout << "Name: " << sneak.name << endl;
	cout << "Weight: " << sneak.weight << endl;
	cout << "cal: " << sneak.cal << endl;

	return 0;

}
