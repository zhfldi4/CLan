#include <iostream>

using namespace std;

class Student {
public:
	char name[20];
	int kor, math, eng;
	float tot, avg;

public:
	void print() {
		cout << "name: " << name << endl;
		cout << "score:\tKor\tMath\tEng\tTotal\tAvg" << endl;
		cout << "\t" << kor << "\t" << math << "\t" << eng << "\t" << tot
				<< "\t" << avg << endl;
	}

};

