#include <stdio.h>
#include <stdlib.h>

int main(void) {

	float x = 1e+33, y = 1e+25, z;

	z = x + y;
	if (x < z)
		printf("%e < %e + %e : True ", x, x, y);
	else
		printf("%e < %e + %e : False ", x, x, y);

	return 0;
}
