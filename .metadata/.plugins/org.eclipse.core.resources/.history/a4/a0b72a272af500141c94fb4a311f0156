#include <stdio.h>

int main(void) {


	int row, column;

	scanf("%d %d", &row, &column);

	int i, j, inc = 1;
	for (i = 0; i < row; i++) {
		for (j = 0; j < column; j++) {
			printf("%d ", inc++);
		}
		printf("\n");
	}




	return 0;
}


