#include <iostream>

using namespace std;

const double AINC = 0.1;
const double BINC = 0.05;

int main() {

	int aMoney, bMoney;
	int aInc, bInc;

	aMoney = bMoney = 100000;
	aInc = 0;
	bInc = bMoney;

	int aTotMoney = aMoney, bTotMoney = bMoney;

	for (int i = 1; i < 6; i++) {
		aInc = (int)(AINC * aMoney);
		bInc = (int)(BINC * bMoney);

	}

	return 0;

}
