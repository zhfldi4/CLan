#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int equality;

	printf("Input num:");
	scanf("%d", &equality);

	int fx = 3 * equality * equality - equality + 20;

	printf("f(%d) = %d", equality, fx);

	return 0;
}
