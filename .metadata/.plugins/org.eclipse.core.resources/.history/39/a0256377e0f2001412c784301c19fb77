#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int a, b, c;
	int x;

	printf("Input a, b, c: ");
	scanf("%d %d %d", &a, &b, &c);
	printf("Input x:");
	scanf("%d", &x);

	int fx = a * x * x + b * x + c;

	printf("f(%d) = %d", x, fx);

	return 0;
}
