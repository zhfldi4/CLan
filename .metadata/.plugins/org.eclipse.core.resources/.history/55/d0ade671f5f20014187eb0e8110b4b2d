#include <stdio.h>
#include <stdlib.h>
#include <math.h>

void f(void);
void g(void);
void h(void);

int main(void) {

	(*f)();

	return 0;
}

void f(void) {
	printf("f() called\n");
	(((*g)))();
}

void g(void) {
	printf("g() called\n");
	(*(*(*h)))();
}

void h(void) {
	printf("h() called\n");
}



