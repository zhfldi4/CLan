#include <stdio.h>

int main(void) {

	puts("No 1.");
	{
		printf("1 2 3 4 5\n");

	}

	puts("No 2.");
	{
		int one = 1, two = 2, three = 3, four = 4, five = 5;

		printf("%d %d %d %d %d\n", one, two ,three, four, five);
	}

	puts("No 3.");
	{
		int one = 1;

		printf("%d %d %d %d %d\n", one++, one++, one++, one++, one++);
	}

	return 0;
}
