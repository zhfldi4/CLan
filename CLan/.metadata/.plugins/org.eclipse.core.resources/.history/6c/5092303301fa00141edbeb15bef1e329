#include <stdio.h>

int main(void) {

	char ch;
	int alphabet[26] = {0,};

	while ((ch = getchar()) != '\n') {
		alphabet[97 - (int)ch]++;
	}

	int i;
	for (i = 0; i < 26; i++) {
		printf("%c\t\t%d\n",(char) 97 + i, alphabet[i]);
	}

	return 0;
}
